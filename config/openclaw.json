{
  "gateway": {
    "mode": "local",
    "port": 18789,
    "controlUi": { "enabled": true }
  },
  "skills": {
    "allowBundled": []
  },
  "agents": {
    "defaults": {
      "workspace": "~/workspace",
      "model": {
        "primary": "ollama/qwen2.5-coder:7b"
      },
      "models": {
        "ollama/qwen2.5-coder:7b": { "alias": "Local Model" }
      },
      "sandbox": {
        "mode": "non-main",
        "scope": "agent",
        "workspaceAccess": "rw",
        "docker": {
          "image": "openclaw-sandbox:bookworm-slim",
          "readOnlyRoot": true,
          "network": "none",
          "user": "1000:1000",
          "capDrop": ["ALL"]
        }
      }
    }
  },
  "models": {
    "mode": "merge",
    "providers": {
      "ollama": {
        "baseUrl": "http://ollama:11434/v1",
        "apiKey": "ollama",
        "api": "openai-responses",
        "models": [
          {
            "id": "qwen2.5-coder:7b",
            "name": "qwen2.5-coder:7b",
            "reasoning": false,
            "input": ["text"],
            "cost": { "input": 0, "output": 0, "cacheRead": 0, "cacheWrite": 0 },
            "contextWindow": 32768,
            "maxTokens": 8192
          }
        ]
      }
    }
  }
}
